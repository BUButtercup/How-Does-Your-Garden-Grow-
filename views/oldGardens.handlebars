<main>
    <h2>Saved Garden Plans</h2>
    <p>Here you can view a saved plan and choose to use, edit, or delete it!</p>
    <div id="gardenBox"></div>
    <ul id="gardenList" style="margin: 10px; padding: 10px; width: 100%; height: 150px; overflow-y: auto">
        {{#each garden}}
            <li>{{garden.name}}<button>Use</button><button>Edit</button><button>Delete</button></li>
        {{/each}}
    </ul>
</main>